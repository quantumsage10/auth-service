{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/tests/users/user.spec.ts",
            "outFiles": ["${workspaceFolder}/**/*.js"]
        },

        {
            "type": "node",
            "request": "attach",
            "name": "Attach to Program",
            "processId": "${command:PickProcess}",
            "skipFiles": ["<node_internals>/**"],
            "outFiles": ["${workspaceFolder}/**/*.js"]
        }
    ]
}
